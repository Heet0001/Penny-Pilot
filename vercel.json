{
  "version": 2,
  "builds": [
    {
      "src": "backend/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "frontend/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    },
    {
      "src": "/Dashboard/(.*)",
      "dest": "/frontend/Dashboard/$1"
    },
    {
      "src": "/Debts/(.*)",
      "dest": "/frontend/Debts/$1"
    },
    {
      "src": "/Investment/(.*)",
      "dest": "/frontend/Investment/$1"
    },
    {
      "src": "/Loginpage/(.*)",
      "dest": "/frontend/Loginpage/$1"
    },
    {
      "src": "/MoneyTransfer/(.*)",
      "dest": "/frontend/MoneyTransfer/$1"
    },
    {
      "src": "/utils/(.*)",
      "dest": "/frontend/utils/$1"
    }
  ]
}
